generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Services {
  id        Int     @id @default(autoincrement())
  title     String  @unique
  content   String
  price     Float
  whatsapp  String
  image     String
  published Boolean @default(false)
}
