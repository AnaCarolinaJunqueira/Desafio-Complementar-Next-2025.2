generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Services {
  id        Int     @id @default(autoincrement())
  title     String  @unique
  content   String
  price     Decimal @db.Decimal(6, 2)
  whatsapp  String
  image     String
  published Boolean @default(false)
}
