generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Services {
  id        Int     @id @default(autoincrement())
  title     String  @unique
  content   String
  price     Decimal @db.Decimal(6, 2)
  whatsapp  String
  image     String
  published Boolean @default(false)
}
